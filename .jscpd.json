{
  "absolute": true,
  "blame": true,
  "format": ["css", "javascript", "json", "tsx", "typescript"],
  "gitignore": true,
  "ignoreCase": true,
  "maxLines": 5000,
  "minLines": 3,
  "mode": "strict",
  "noSymlinks": false,
  "output": "./reports/jscpd/",
  "reporters": ["html", "json"],
  "silent": true,
  "store": "leveldb",
  "threshold": 0
}
